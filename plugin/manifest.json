{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "storage",
      "webRequest",
      "tabs",
      "nativeMessaging"
    ],
    "background": {
      "service_worker": 
        "src/index.js"
    },
    "host_permissions": [
      "*://*.google.com/"
    ],
    
    "action": {
      "default_popup": "index.html",
      "default_icon": "128.png"
    },
    "devtools_page": "devtools.html"
  }