{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "storage",
      "webRequest",
      "nativeMessaging"
    ],
    "host_permissions": [
      "*://*.google.com/"
    ],
    "background": {
    "service_worker": "src/index.js",
    "type": "module"
  },

    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon/icon_16.png",
        "32": "icon/icon_32.png",
        "64": "icon/icon_64.png",
        "128": "icon/icon_128.png"}
    },
    "icons": {
      "16": "icon/icon_16.png",
      "32": "icon/icon_32.png",
      "64": "icon/icon_64.png",
      "128": "icon/icon_128.png"}

  }
