{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "declarativeContent",
      "storage",
      "webRequest"
    ],
    "host_permissions": [
      "*://*.google.com/"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": "128.png"
    },
    "devtools_page": "devtools.html"
  }