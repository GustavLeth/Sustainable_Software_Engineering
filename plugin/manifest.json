{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "webRequest",
      "nativeMessaging"
    ],
    "host_permissions": [
      "*://*.google.com/"
    ],

    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon/icon16.png",
        "32": "icon/icon32.png",
        "64": "icon/icon64.png",
        "128": "icon/icon128.png"}
    },

    "devtools_page": "devtools.html",

    "icons": {
      "16": "icon/icon16.png",
      "32": "icon/icon32.png",
      "64": "icon/icon64.png",
      "128": "icon/icon128.png"}

  }
