{
    "manifest_version": 3,
    "name": "Carbon emission tracker",
    "description": "This is an extension that tracks your carbon usage by monitoring HTTP content length headers and the carbon intensity of the grid in your area, and the servers area.",
    "version": "1.0",
    "permissions": [
      "storage",
      "webRequest"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "background": {
    "service_worker": "src/ServiceWorker.js",
    "type": "module"
  },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "16.png",
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"}
    },
    "icons": {
      "16": "16.png",
      "32": "32.png",
      "64": "64.png",
      "128": "128.png"}

  }
